<html>
<head>
    
    <meta http-equiv="PRAGMA" content="NO-CACHE" />
    <meta http-equiv="EXPIRES" content="Mon, 22 Jul 2002 11:12:01 GMT" />
    <meta http-equiv="Content-Language" content="en-us" />
    <meta http-equiv="content-type" content="text/html; charset=us-ascii" />
    <link rel="stylesheet" type="text/css" href="styles/hplip.css" />
    <script type="text/javascript" src="scripts/hplip.js"></script>
    <script type="text/javascript" src="scripts/xml_loader.js"></script>
    <script type="text/javascript">
        <!--
        var divText = "";
        #set $l = len($missing_optional_dependencies)
        var len = $l
        #for $d in $missing_optional_dependencies
            divText += "$missing_optional_dependencies[$d]<br/>";
        #end for
        var divID = 'notes_field';
       
        function todoOnLoad() {
            header = '$header_string'; 
        subheader = '$sub_header_string'; 
        generateHeader(header, subheader);
            getContentText(divText, divID);
        };
        -->
        </script>

<title>$installer_title</title>
</head>

<body onload="todoOnLoad();">
<!-- Header Div -->    
<div id="header_box" class="background_box"></div>

<!-- Body Div -->
<div class="body_content">
    <div id="message_title">install_optional_message_title</div>

    <div id="message_body">
        <div id="notes_field"></div>
    </div>

    <div id="message_footer">$install_optional_message_footer</div>
</div>

<!-- Button Div -->
<div class="button_options">

    <div id="quit_button" OnSelectStart='return false;' onClick='todoOnQuit("quit");' >
      <img src="../images/button1.png" width="100" height="35">
      <div id="quitHiliteText">$quit_button</div>
      <div id="quitText">$quit_button</div>
    </div>

  <div id="previous_button" OnSelectStart='return false;' onClick='indexClickActions( "previous_button", "turn_off_options");' >
    <img src="../images/button1.png" width="100" height="35">
    <div id="noHiliteText">$no_button</div>
    <div id="noText">$no_button</div>
  </div>

  <div id="next_button" OnSelectStart='return false;' onClick='indexClickActions( "next_button", "install_optional_controller");' >
    <img src="../images/button1.png" width="100" height="35">
    <div id="yesHiliteText">$yes_button</div>
    <div id="yesText">$yes_button</div>
  </div>
</div>

</body>
</html>
